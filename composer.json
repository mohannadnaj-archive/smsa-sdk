{
    "name": "mohannadnaj/smsa-sdk",
    "description": "Simple PHP package for integrating SMSA Web Services (SECOM).",
    "type": "library",
    "keywords": [
        "SMSA",
        "SmsaExpress",
        "SmsaSDK"
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "wsdl2phpgenerator/wsdl2phpgenerator": "^3.4"
    },
    "license": "MIT",
    "homepage": "https://github.com/MohannadNaj/smsa-sdk",
    "support": {
        "issues": "https://github.com/MohannadNaj/smsa-sdk/issues",
        "source": "https://github.com/MohannadNaj/smsa-sdk"
    },
    "authors": [
        {
            "name": "MohannadNaj",
            "email": "mohannadnaj@me.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SmsaSDK\\": "src/",
            "SmsaSDKTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {
                "Smsa": "SmsaSDK\\Smsa"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-soap": "*"
    }
}
